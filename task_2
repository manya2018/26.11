from random import randint
A=[]
for i in range(10):
    A.append(randint(-1100,1100))
print(A)
b=sorted(A)
def bin(A):
    n=len(A)
    p=0
    x=int(input())
    while x!=A[len(A)/2]:
        r=(n-p)/2
        if A[r]>0:
            p=r 
        if A[r]<0:
            n=r
    return print(r)        
print(bin(b))
